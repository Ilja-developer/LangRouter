{
  "name": "LangRouter",
  "lowCaseName": "langrouter",
  "description": "Language Router for Babel",
  "author": "gadamiak",
  "version": "1.0.1",
  "package": {
    "elements": {
      "plugins": [
        {
          "name": "LangRouter",
          "file": "langrouter.plugin.php",
          "events": ["OnHandleRequest"]
        }
      ]
    },
    "systemSettings": [
      {
        "key": "debug",
        "type": "combo-boolean",
        "area": "system",
        "value": "0"
      }
    ]
  },
  "build": {
    "setupOptions": {
      "source": "resolvers/langrouter.options.php"
    },
    "resolver": {
      "after": ["langrouter.set_options.php"]
    }
  }
}